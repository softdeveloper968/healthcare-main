<SfSpinner Size="40" Type="SpinnerType.Bootstrap4" @bind-Visible="isLoading"></SfSpinner>

@code {
    string Content = "New";
    SfGrid<DiagnosisViewModel> diagnosisGrid;
    SfGrid<WoundViewModel> updateWoundGrid;
    int SelectedPage { get; set; } = 0;
    AdultAssessmentRequestModel intitalAssessmentRequestModel = new AdultAssessmentRequestModel();
    WoundViewModel woundRequestModel = new WoundViewModel();
    DiagnosisViewModel diagnosisRequestViewModel = new DiagnosisViewModel();
    bool isLoading = false;
    [Parameter]
    public int Id { get; set; }
    [Parameter]
    public bool View { get; set; }
    [Parameter]
    public string Role { get; set; }
    SfDialog diagnosis;
    SfDialog woundDialog;

    protected override async Task OnInitializedAsync()
    {
        isLoading = true;
        var responseMessage = await Http.GetAsync(wsp.Path + $"AdultAssessment/GetAssessmentById?id={Id}");
        var response = responseMessage.Content.ReadAsStringAsync().Result;
        var result = (JsonConvert.DeserializeObject<Response<AdultAssessmentRequestModel>>(response));
        if (result.Status == "Success")
        {
            intitalAssessmentRequestModel = result.Data;
        }
        else
        {
            toastService.ShowError(result.Message);
            NavigateToAssessmentGrid();
        }
        isLoading = false;
        StateHasChanged();
    }

    public void GoToLivingArrangements()
    {
        SelectedPage = 2;
        StateHasChanged();
    }

    public void GoToDiagnosis()
    {
        SelectedPage = 1;
        StateHasChanged();
    }

    public void GoToSkilledAssessments()
    {
        SelectedPage = 3;
        StateHasChanged();
    }


    public void GoToBradenScale()
    {
        SelectedPage = 4;
        StateHasChanged();
    }

    public void GoToMAHC()
    {
        SelectedPage = 5;
        StateHasChanged();
    }

    public void GoToInterventions()
    {
        SelectedPage = 6;
        StateHasChanged();
    }

    public void GoTOPlanofCare()
    {
        SelectedPage = 7;
        StateHasChanged();
    }

    public void GoToCarePlanning()
    {
        SelectedPage = 8;
        StateHasChanged();
    }

    public void NavigateToAssessmentGrid()
    {
        if (Role == UserRoles.Nurse.ToString())
            navigation.NavigateTo("nurse/assessments");
        else
            navigation.NavigateTo("agency/assessments");
    }

    public async Task UpdateAssessment()
    {
        isLoading = true;
        var responseMessage = await Http.PostAsJsonAsync(wsp.Path + "AdultAssessment/UpdateAssessment", intitalAssessmentRequestModel);
        var response = responseMessage.Content.ReadAsStringAsync().Result;
        var result = (JsonConvert.DeserializeObject<Response<string>>(response));
        if (result.Status == "Success")
        {
            toastService.ShowSuccess(result.Message);
            NavigateToAssessmentGrid();
        }
        else
        {
            toastService.ShowError(result.Message);
        }
        isLoading = false;
        StateHasChanged();
    }

    public void OnDiagnosisNewButtonclick()
    {
        diagnosisRequestViewModel = new DiagnosisViewModel();
        diagnosisRequestViewModel.AdultAssessmentId = Id;
        diagnosis.Show();
    }

    public async Task DiagnosisSearchValueChange(Syncfusion.Blazor.Inputs.ChangedEventArgs args)
    {
        await diagnosisGrid.Search(args.Value);
    }

    public async Task UpdateWoundSearchValueChange(Syncfusion.Blazor.Inputs.ChangedEventArgs args)
    {
        await updateWoundGrid.Search(args.Value);
    }

    public void OnUpdateWoundNewButtonclick()
    {
        woundRequestModel = new WoundViewModel();
        woundRequestModel.AdultAssessmentId = Id;
        woundDialog.Show();
    }

    public void CloseDiagnosisDialog()
    {
        diagnosis.Hide();
    }

    public void UpdateDiagnosis()
    {
        Random rnd = new Random();
        if (diagnosisRequestViewModel.Id == 0)
        {
            diagnosisRequestViewModel.Id = rnd.Next();
            intitalAssessmentRequestModel.Diagnosis.Add(diagnosisRequestViewModel);
        }
        else
        {
            if (intitalAssessmentRequestModel.Diagnosis.FirstOrDefault(x => x.Id == diagnosisRequestViewModel.Id) != null)
            {
                intitalAssessmentRequestModel.Diagnosis.FirstOrDefault(x => x.Id == diagnosisRequestViewModel.Id).DxDescription = diagnosisRequestViewModel.DxDescription;
                intitalAssessmentRequestModel.Diagnosis.FirstOrDefault(x => x.Id == diagnosisRequestViewModel.Id).DiagnosisCode = diagnosisRequestViewModel.DiagnosisCode;
                intitalAssessmentRequestModel.Diagnosis.FirstOrDefault(x => x.Id == diagnosisRequestViewModel.Id).DxDate = diagnosisRequestViewModel.DxDate;
                intitalAssessmentRequestModel.Diagnosis.FirstOrDefault(x => x.Id == diagnosisRequestViewModel.Id).IsOtherDx = diagnosisRequestViewModel.IsOtherDx;
                intitalAssessmentRequestModel.Diagnosis.FirstOrDefault(x => x.Id == diagnosisRequestViewModel.Id).IsPrimaryDx = diagnosisRequestViewModel.IsPrimaryDx;
                intitalAssessmentRequestModel.Diagnosis.FirstOrDefault(x => x.Id == diagnosisRequestViewModel.Id).Rating = diagnosisRequestViewModel.Rating;
            }
        }
        CloseDiagnosisDialog();
        diagnosisGrid.Refresh();
        StateHasChanged();
    }

    public void RemoveDiagnosis(int id)
    {
        var diagnosisData = intitalAssessmentRequestModel.Diagnosis.FirstOrDefault(x => x.Id == id);
        if (diagnosisData != null)
        {
            intitalAssessmentRequestModel.Diagnosis.Remove(diagnosisData);
        }
        diagnosisGrid.Refresh();
        StateHasChanged();
    }

    public void OpenEditDiagnosisPopup(DiagnosisViewModel model)
    {
        diagnosisRequestViewModel = model;
        diagnosis.Show();
        StateHasChanged();
    }

    public void CloseWoundDialog()
    {
        woundDialog.Hide();
    }

    public void UpdateWound()
    {
        Random rnd = new Random();
        if (woundRequestModel.Id == 0)
        {
            woundRequestModel.Id = rnd.Next();
            intitalAssessmentRequestModel.Wound.Add(woundRequestModel);
        }
        else
        {
            var woundData = intitalAssessmentRequestModel.Wound.FirstOrDefault(x => x.Id == woundRequestModel.Id);
            if (woundData != null)
            {
                intitalAssessmentRequestModel.Wound.Remove(woundData);
                intitalAssessmentRequestModel.Wound.Add(woundRequestModel);
            }
        }
        CloseWoundDialog();
        updateWoundGrid.Refresh();
        StateHasChanged();
    }

    public void RemoveWound(int id)
    {
        var woundData = intitalAssessmentRequestModel.Wound.FirstOrDefault(x => x.Id == id);
        if (woundData != null)
        {
            intitalAssessmentRequestModel.Wound.Remove(woundData);
        }
        updateWoundGrid.Refresh();
        StateHasChanged();
    }

    public void OpenEditWoundPopup(WoundViewModel model)
    {
        woundRequestModel = model;
        woundDialog.Show();
        StateHasChanged();
    }
}
