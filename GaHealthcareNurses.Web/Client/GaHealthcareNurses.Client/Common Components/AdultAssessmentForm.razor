@using Syncfusion.Blazor.Inputs
@using Syncfusion.Blazor.Grids
@using Syncfusion.Blazor.Popups
@using GaHealthcareNurses.Entity.ViewModels
@using GaHealthcareNurses.Entity.Common
@using GaHealthcareNurses.Entity.Models
@using Syncfusion.Blazor.Buttons
@using Syncfusion.Blazor.Calendars
@using Syncfusion.Blazor.Navigations
@using Newtonsoft.Json
@using Syncfusion.Blazor.Spinner
@inject NavigationManager navigation
@inject HttpClient Http
@inject GaHealthcareNurses.Client.NewFolder.WebServicePath wsp
@inject IToastService toastService

<div class="form_input">
    <EditForm Model="@intitalAssessmentRequestModel" OnValidSubmit="UpdateAssessment">
        <DataAnnotationsValidator />
        <SfTab @bind-SelectedItem="SelectedPage" LoadOn="ContentLoad.Demand" CssClass="modal_SfTabs">
            <TabItems>
                <TabItem>
                    <ChildContent>
                        <TabHeader Text="Patient Details"></TabHeader>
                    </ChildContent>
                    <ContentTemplate>
                        <div>
                            <div>
                                <h5 class="text-center">Adult Assessment</h5>
                                <h5 class="text-right">Date:@DateTime.Now.ToString("MM/dd/yyyy")</h5>
                            </div>
                            <div>
                                <div class="form-box">
                                    <div>
                                        <div class="d_flex">
                                            <label class="no_wrap m-0 pr_10">Billing Code:</label>
                                            <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@intitalAssessmentRequestModel.BillingCode" />
                                        </div>
                                        <ValidationMessage For="@(() => intitalAssessmentRequestModel.BillingCode)" />
                                    </div>
                                    <div class="d_flex">
                                        <label class="no_wrap m-0 pr_10">Time in:</label>
                                        <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@intitalAssessmentRequestModel.TimeIn" />
                                    </div>
                                    <div class="d_flex">
                                        <label class="no_wrap m-0 pr_10">Time Out:</label>
                                        <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@intitalAssessmentRequestModel.TimeOut" />
                                    </div>
                                </div>
                                <div class="row mb_10">
                                    <div class="col-md-2">
                                        <label>Patient Name:</label>
                                    </div>
                                    <div class="col-md-2">
                                        <label>Last: @intitalAssessmentRequestModel.LastName</label>
                                    </div>
                                    <div class="col-md-2">
                                        <label>First: @intitalAssessmentRequestModel.FirstName</label>
                                    </div>
                                    @*<div class="col-md-2">
                                            <label>MR Number:</label>
                                        </div>*@
                                    <div class="col-md-2">
                                        <label>Gender: @intitalAssessmentRequestModel.Gender</label>
                                    </div>
                                    <div class="col-md-2">
                                        <label>Age: @intitalAssessmentRequestModel.Age</label>
                                    </div>
                                </div>
                                <div class="row mb_10">
                                    <div class="col-md-3 d_flex">
                                        <label class="no_wrap m-0 pr_10">Location Name:</label>
                                        <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@intitalAssessmentRequestModel.LocationName" />
                                    </div>
                                    <div class="col-md-3 d_flex">
                                        <label class="no_wrap m-0 pr_10">Location Phone:</label>
                                        <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@intitalAssessmentRequestModel.LocationPhone" />
                                    </div>
                                </div>
                                <div class="row mb_10">
                                    <div class="col-md-4">
                                        <div class="d_flex">
                                            <label class="no_wrap m-0 pr_10">Start Care of Date:</label>
                                            <SfDatePicker Placeholder="MM/DD/YYYY" ShowClearButton="true" @bind-Value="@intitalAssessmentRequestModel.StartOfCaredate"></SfDatePicker>
                                        </div>

                                        <ValidationMessage For="@(() => intitalAssessmentRequestModel.StartOfCaredate)" />
                                    </div>
                                    <div class="col-md-4">
                                        <div class="d_flex">
                                            <label class="no_wrap m-0 pr_10">Certification Period From:</label>
                                            <SfDatePicker Placeholder="MM/DD/YYYY" ShowClearButton="true" @bind-Value="@intitalAssessmentRequestModel.CertificationFromDate"></SfDatePicker>
                                        </div>
                                        <ValidationMessage For="@(() => intitalAssessmentRequestModel.CertificationFromDate)" />
                                    </div>
                                    <div class="col-md-4">
                                        <div class="d_flex">
                                            <label class="no_wrap m-0 pr_10">To:</label>
                                            <SfDatePicker Placeholder="MM/DD/YYYY" ShowClearButton="true" @bind-Value="@intitalAssessmentRequestModel.CertificationToDate"></SfDatePicker>
                                        </div>
                                        <ValidationMessage For="@(() => intitalAssessmentRequestModel.CertificationToDate)" />
                                    </div>
                                </div>
                                <div>
                                    <div class="row mb_10">
                                        <div class="col-md-3">
                                            <label>Race/Ethnicity (check all that apply):</label>
                                        </div>
                                        <div class="col-md-3">
                                            <SfCheckBox Label="American Indian or Alaska Native" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.IsAmericanIndian"></SfCheckBox>
                                        </div>
                                        <div class="col-md-3">
                                            <SfCheckBox Label="Asian" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.IsAsian"></SfCheckBox>
                                        </div>
                                        <div class="col-md-3">
                                            <SfCheckBox Label="Black or African American" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.IsBlackAmerican"></SfCheckBox>
                                        </div>
                                    </div>
                                    <div class="row mb_10">
                                        <div class="col-md-3">
                                            <SfCheckBox Label="Hispanic or Latino " LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.IsHispanic"></SfCheckBox>
                                        </div>
                                        <div class="col-md-3">
                                            <SfCheckBox Label="Native Hawaiian or other Pacific Islander" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.IsNativeHawaiian"></SfCheckBox>
                                        </div>
                                        <div class="col-md-3">
                                            <SfCheckBox Label="White" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.IsWhite"></SfCheckBox>
                                        </div>
                                    </div>
                                </div>
                                <div class="row mb_25">
                                    <div class="col-md-4">
                                        <div class="d_flex">
                                            <label class="no_wrap m-0 pr_10">Referring Physician:</label>
                                            <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@intitalAssessmentRequestModel.ReferringPhysician" />
                                        </div>
                                        <ValidationMessage For="@(() => intitalAssessmentRequestModel.ReferringPhysician)" />

                                    </div>
                                    <div class="col-md-4">
                                        <div class="d_flex">
                                            <label class="no_wrap m-0 pr_10">Address:</label>
                                            <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@intitalAssessmentRequestModel.PhysicianAddress" />
                                        </div>
                                        <ValidationMessage For="@(() => intitalAssessmentRequestModel.PhysicianAddress)" />
                                    </div>
                                    <div class="col-md-4">
                                        <div class="d_flex">
                                            <label class="no_wrap m-0 pr_10">Phone:</label>
                                            <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@intitalAssessmentRequestModel.PhysicianPhone" />
                                        </div>
                                        <ValidationMessage For="@(() => intitalAssessmentRequestModel.PhysicianPhone)" />
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="create_account">
                            <div class="ds_btns">
                                <button type="button" @onclick="GoToDiagnosis" class="create_account btn-theme border ">
                                    Next
                                </button>
                            </div>
                        </div>
                    </ContentTemplate>
                </TabItem>
                <TabItem>
                    <ChildContent>
                        <TabHeader Text="Diagnosis"></TabHeader>
                    </ChildContent>
                    <ContentTemplate>
                        <SfTab>
                            <TabItems>
                                <TabItem>
                                    <ChildContent>
                                        <TabHeader Text="Patient History"></TabHeader>
                                    </ChildContent>
                                    <ContentTemplate>
                                        <div>
                                            <div>
                                                <div>
                                                    <h3>Patient History</h3>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-2">
                                                        <label>Recent Hospitalization:</label>
                                                    </div>
                                                    <div class="d_flex">
                                                        <div class="pr_10">
                                                            <SfRadioButton Label="No" Value="No" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.IsRecentHosp"></SfRadioButton>
                                                        </div>
                                                        <div class="">
                                                            <SfRadioButton Label="Yes" Value="Yes" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.IsRecentHosp"></SfRadioButton>
                                                        </div>
                                                    </div>

                                                </div>
                                                <div class="mb_10">
                                                    <div class="row mb_5">
                                                        <div class="col-md-3 d_flex">
                                                            <label class="no_wrap">Date:</label>
                                                            <SfDatePicker Placeholder="MM/DD/YYYY" ShowClearButton="true" @bind-Value="@intitalAssessmentRequestModel.RecentHospDate1"></SfDatePicker>
                                                        </div>
                                                        <div class="col-md-6 d_flex">
                                                            <label class="no_wrap">Reason:</label>
                                                            <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@intitalAssessmentRequestModel.RecentHospReason1" />
                                                        </div>
                                                    </div>
                                                    <div class="row mb_5">
                                                        <div class="col-md-3 d_flex">
                                                            <label class="no_wrap">Date:</label>
                                                            <SfDatePicker Placeholder="MM/DD/YYYY" ShowClearButton="true" @bind-Value="@intitalAssessmentRequestModel.RecentHospDate2"></SfDatePicker>
                                                        </div>
                                                        <div class="col-md-6 d_flex">
                                                            <label class="no_wrap">Reason:</label>
                                                            <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@intitalAssessmentRequestModel.RecentHospReason2" />
                                                        </div>
                                                    </div>
                                                    <div class="row mb_5">
                                                        <div class="col-md-3 d_flex">
                                                            <label class="no_wrap">Date:</label>
                                                            <SfDatePicker Placeholder="MM/DD/YYYY" ShowClearButton="true" @bind-Value="@intitalAssessmentRequestModel.RecentHospDate3"></SfDatePicker>
                                                        </div>
                                                        <div class="col-md-6 d_flex">
                                                            <label class="no_wrap">Reason:</label>
                                                            <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@intitalAssessmentRequestModel.RecentHospReason3" />
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="mb_10">
                                                    <label>Last Physician contact/appointment</label>
                                                    <div class="row mb_5">
                                                        <div class="col-md-3 d_flex">
                                                            <label class="no_wrap">Date:</label>
                                                            <SfDatePicker Placeholder="MM/DD/YYYY" ShowClearButton="true" @bind-Value="@intitalAssessmentRequestModel.LastContactDate"></SfDatePicker>
                                                        </div>
                                                        <div class="col-md-6 d_flex">
                                                            <label class="no_wrap">Reason:</label>
                                                            <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@intitalAssessmentRequestModel.LastContactReason" />
                                                        </div>
                                                    </div>

                                                </div>
                                            </div>
                                            <fieldset class="fieldset_box">
                                                <legend>Immunizations</legend>
                                                <table class="table custom_table">
                                                    <tbody>
                                                        <tr>
                                                            <td><SfCheckBox Label="Pneumococcal Pneumonia" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.IsPneumonia"></SfCheckBox></td>
                                                            <td>
                                                                <SfRadioButton Label="Unknown" Value="Unknown" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.Pneumonia"></SfRadioButton>
                                                                <SfRadioButton Label="No" Value="No" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.Pneumonia"></SfRadioButton>
                                                                <SfRadioButton Label="Yes" Value="Yes" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.Pneumonia"></SfRadioButton>
                                                            </td>
                                                            <td>
                                                                <div class="d_flex">
                                                                    <label class="no_wrap">Date:</label>
                                                                    <SfDatePicker Placeholder="MM/DD/YYYY" ShowClearButton="true" @bind-Value="@intitalAssessmentRequestModel.PneumoniaDate"></SfDatePicker>
                                                                </div>

                                                            </td>
                                                            <td>
                                                                <div class="d_flex">
                                                                    <label class="no_wrap">Location:</label>
                                                                    <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@intitalAssessmentRequestModel.PneumoniaLocation" />
                                                                </div>
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td> <SfCheckBox Label="Seasonal Influenza:" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.IsInfluenza"></SfCheckBox></td>
                                                            <td>
                                                                <SfRadioButton Label="Unknown" Value="Unknown" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.Influenza"></SfRadioButton>
                                                                <SfRadioButton Label="No" Value="No" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.Influenza"></SfRadioButton>
                                                                <SfRadioButton Label="Yes" Value="Yes" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.Influenza"></SfRadioButton>
                                                            </td>
                                                            <td>
                                                                <div class="d_flex">
                                                                    <label class="no_wrap">Date:</label>
                                                                    <SfDatePicker Placeholder="MM/DD/YYYY" ShowClearButton="true" @bind-Value="@intitalAssessmentRequestModel.InfluenzaDate"></SfDatePicker>
                                                                </div>
                                                            </td>
                                                            <td>
                                                                <div class="d_flex">
                                                                    <label class="no_wrap">Location:</label>
                                                                    <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@intitalAssessmentRequestModel.InfluenzaLocation" />
                                                                </div>
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td>
                                                                <SfCheckBox Label="Shingle" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.IsShingle"></SfCheckBox>
                                                            </td>
                                                            <td>
                                                                <SfRadioButton Label="Unknown" Value="Unknown" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.Shingle"></SfRadioButton>
                                                                <SfRadioButton Label="No" Value="No" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.Shingle"></SfRadioButton>
                                                                <SfRadioButton Label="Yes" Value="Yes" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.Shingle"></SfRadioButton>
                                                            </td>
                                                            <td>
                                                                <div class="d_flex">
                                                                    <label class="no_wrap">Date:</label>
                                                                    <SfDatePicker Placeholder="MM/DD/YYYY" ShowClearButton="true" @bind-Value="@intitalAssessmentRequestModel.ShingleDate"></SfDatePicker>
                                                                </div>
                                                            </td>
                                                            <td>
                                                                <div class="d_flex">
                                                                    <label class="no_wrap">Location:</label>
                                                                    <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@intitalAssessmentRequestModel.ShingleLocation" />
                                                                </div>
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td><SfCheckBox Label="Tetanus" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.IsTetanus"></SfCheckBox></td>
                                                            <td>
                                                                <SfRadioButton Label="Unknown" Value="Unknown" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.Tetanus"></SfRadioButton>
                                                                <SfRadioButton Label="No" Value="No" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.Tetanus"></SfRadioButton>
                                                                <SfRadioButton Label="Yes" Value="Yes" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.Tetanus"></SfRadioButton>
                                                            </td>
                                                            <td>
                                                                <div class="d_flex">
                                                                    <label class="no_wrap">Date:</label>
                                                                    <SfDatePicker Placeholder="MM/DD/YYYY" ShowClearButton="true" @bind-Value="@intitalAssessmentRequestModel.TetanusDate"></SfDatePicker>
                                                                </div>
                                                            </td>
                                                            <td>
                                                                <div class="d_flex">
                                                                    <label class="no_wrap">Location:</label>
                                                                    <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@intitalAssessmentRequestModel.TetanusLocation" />
                                                                </div>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                                <hr class="my_10" />
                                                <label>Pertinent medical history and reason for home care:</label>
                                                <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@intitalAssessmentRequestModel.ReasonForHomecare" />
                                            </fieldset>
                                            <fieldset class="mt_13">
                                                <div class="d_flex mb_5">
                                                    <label class="no_wrap">Patient aware of Advance Directives: </label>
                                                    <SfRadioButton Label="Yes" Value="Yes" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.IsAwareAdvanceDirective" CssClass="pr_10"></SfRadioButton>
                                                    <SfRadioButton Label="No" Value="No" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.IsAwareAdvanceDirective" CssClass="pr_10"></SfRadioButton>
                                                    <SfRadioButton Label="N/A, no advance directives" Value="N/A, no advance directives" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.IsAwareAdvanceDirective" CssClass="pr_10"></SfRadioButton>
                                                </div>
                                                <div class="d_flex mb_10">
                                                    <label class="no_wrap">Copies of Advance Directives on file at:</label>
                                                    <SfRadioButton Label="Patient's home" Value="Patient's home" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.IsADCopiesAtHome" CssClass="pr_10"></SfRadioButton>
                                                    <div class="d_flex">
                                                        <SfRadioButton Label="Other" Value="Other" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.IsADCopiesOther" CssClass="pr_10"></SfRadioButton>
                                                        <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@intitalAssessmentRequestModel.ADCopiesOther" />
                                                    </div>
                                                </div>
                                                <div class="row mb_10">
                                                    <div class="col-md-3 d_flex">
                                                        <label class="no_wrap">label:</label>
                                                        <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@intitalAssessmentRequestModel.ADLabel1" />
                                                    </div>
                                                    <div class="col-md-5 d_flex">
                                                        <label class="no_wrap">Description:</label>
                                                        <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@intitalAssessmentRequestModel.ADDescription1" />
                                                    </div>
                                                    <div class="col-md-4 d_flex">
                                                        <label class="no_wrap">Effective Date:</label>
                                                        <SfDatePicker Placeholder="MM/DD/YYYY" ShowClearButton="true" @bind-Value="@intitalAssessmentRequestModel.ADEffectiveDate1"></SfDatePicker>
                                                    </div>
                                                </div>
                                                <div class="row mb_10">
                                                    <div class="col-md-3 d_flex">
                                                        <label class="no_wrap">label:</label>
                                                        <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@intitalAssessmentRequestModel.ADLabel2" />
                                                    </div>
                                                    <div class="col-md-5 d_flex">
                                                        <label class="no_wrap">Description:</label>
                                                        <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@intitalAssessmentRequestModel.ADDescription2" />
                                                    </div>
                                                    <div class="col-md-4 d_flex">
                                                        <label class="no_wrap">Effective Date:</label>
                                                        <SfDatePicker Placeholder="MM/DD/YYYY" ShowClearButton="true" @bind-Value="@intitalAssessmentRequestModel.ADEffectiveDate2"></SfDatePicker>
                                                    </div>
                                                </div>
                                            </fieldset>
                                        </div>
                                    </ContentTemplate>
                                </TabItem>
                                <TabItem>
                                    <ChildContent>
                                        <TabHeader Text="Diagnosis"></TabHeader>
                                    </ChildContent>
                                    <ContentTemplate>
                                        <fieldset>
                                            <p>
                                                Diagnoses(Sequencing of diagnosis should reflect the seriousness of each condition and support the disciplines and services provided)
                                                ICD-10-CM and symptom control rating for each condition. Note that the sequencing of these ratings may not match sequencing of the diagnoses.
                                            </p>
                                            <div class="summaries_data">
                                                <SfGrid @ref="diagnosisGrid" DataSource="intitalAssessmentRequestModel.Diagnosis" AllowPaging="true" AllowSorting="true">
                                                    <SfToolbar>
                                                        <ToolbarItems>
                                                            <ToolbarItem Type="ItemType.Input" Align="Syncfusion.Blazor.Navigations.ItemAlign.Right">
                                                                <Template>
                                                                    <SfTextBox ValueChange="DiagnosisSearchValueChange" Placeholder="Search"></SfTextBox>
                                                                    <span class="e-search-icon e-icons" />
                                                                </Template>
                                                            </ToolbarItem>
                                                            @*<ToolbarItem Type="ItemType.Input" Align="Syncfusion.Blazor.Navigations.ItemAlign.Left">
                                                                <Template>
                                                                    <SfButton OnClick="OnDiagnosisNewButtonclick">New</SfButton>
                                                                </Template>
                                                            </ToolbarItem>*@
                                                        </ToolbarItems>
                                                    </SfToolbar>
                                                    <GridColumns>
                                                        <GridColumn Field="@nameof(DiagnosisViewModel.DxDescription)" Width="78" HeaderText="Description" TextAlign="TextAlign.Left"></GridColumn>
                                                        <GridColumn Field="@nameof(DiagnosisViewModel.DiagnosisCode)" Width="78" HeaderText="DX Code" TextAlign="TextAlign.Left"></GridColumn>
                                                        <GridColumn Field="@nameof(DiagnosisViewModel.Rating)" Width="78" HeaderText="Symptom Control Rating" TextAlign="TextAlign.Left"></GridColumn>
                                                        <GridColumn Field="@nameof(DiagnosisViewModel.DxDate)" Width="78" HeaderText="Date" TextAlign="TextAlign.Left"></GridColumn>
                                                        <GridColumn Width="20" TextAlign="TextAlign.Right" CustomAttributes="@(new Dictionary<string, object>(){ { "class", "actions_btns" }})">
                                                            <Template Context="_Context">
                                                                @{
                                                                    var diagnosis = (_Context as DiagnosisViewModel);

                                                                    <div class="dropdown text-right">
                                                                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-expanded="false"><i class="fas fa-ellipsis-v"></i></a>
                                                                        <ul class="dropdown-menu animated">
                                                                            <li><a href="javascript:void(0)" @onclick="() => OpenEditDiagnosisPopup(diagnosis)"><i class="fas fa-eye"></i> Edit</a></li>
                                                                            <li><a href="javascript:void(0)" @onclick="() => RemoveDiagnosis(diagnosis.Id)"><i class="fas fa-trash-alt"></i> Delete</a></li>
                                                                        </ul>
                                                                    </div>
                                                                }
                                                            </Template>
                                                        </GridColumn>
                                                    </GridColumns>
                                                </SfGrid>
                                                <SfTooltip Target="#target" Content="@Content" Position="Position.TopCenter">
                                                    <SfButton ID="target" CssClass="e-round plus_icons" IsPrimary="true" @onclick="OnDiagnosisNewButtonclick"><i class="fas fa-plus"></i></SfButton>
                                                </SfTooltip>
                                            </div>
                                        </fieldset>
                                    </ContentTemplate>
                                </TabItem>
                            </TabItems>
                        </SfTab>
                    </ContentTemplate>
                </TabItem>
                <TabItem>
                    <ChildContent>
                        <TabHeader Text="Living Arrangements"></TabHeader>
                    </ChildContent>
                    <ContentTemplate>
                        <SfTab>
                            <TabItems>
                                <TabItem>
                                    <ChildContent>
                                        <TabHeader Text="Living Arrangements"></TabHeader>
                                    </ChildContent>
                                    <ContentTemplate>
                                        <div>
                                            <div>
                                                <h3>Living Arrangments</h3>
                                            </div>
                                            <div class="d_flex mb_5">
                                                <label class="no_wrap">Marital Status:</label>
                                                <SfRadioButton CssClass="pr_10" Label="Single" Value="Single" LabelPosition="LabelPosition.After" @bind-Checked="@intitalAssessmentRequestModel.MaritalStatus"></SfRadioButton>
