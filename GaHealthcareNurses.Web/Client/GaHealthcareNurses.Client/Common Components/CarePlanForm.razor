@inject Blazored.LocalStorage.ILocalStorageService localStorage
@inject IToastService toastService
@inject NavigationManager navigation
@using GaHealthcareNurses.Entity.ViewModels;
@using GaHealthcareNurses.Entity.Models;
@inject HttpClient Http
@inject GaHealthcareNurses.Client.NewFolder.WebServicePath wsp
@using Newtonsoft.Json;
@inject IJSRuntime jsRunTime
@using Microsoft.JSInterop
@using Syncfusion.Blazor.Spinner
@using Syncfusion.Blazor.Buttons
@using Syncfusion.Blazor.Inputs
@using Syncfusion.Blazor.Calendars
@using Syncfusion.Blazor.Navigations
@using GaHealthcareNurses.Entity.Common;
@inject Microsoft.AspNetCore.Components.NavigationManager UriHelper
@inject IJSRuntime jsRunTime

<EditForm Model="@carePlanRequestModel" OnValidSubmit="UpdateCarePlan">
    <div class="text-left">
        <div class="e-wrapper">
            <DataAnnotationsValidator />
            <div class="address_box_div">
                <h5 class="text-right">Date:@DateTime.Now.ToString("MM/dd/yyyy")</h5>
                <div class="row gap_10">
                    <div class="col-md-6">
                        <h3>Patient's Name and Address</h3>
                        <p>@carePlanRequestModel.PatientName</p>
                        <p>@carePlanRequestModel.PatientAddress</p>
                        <p>Age: @carePlanRequestModel.PatientAge</p>
                    </div>
                    <div class="col-md-6">
                        <h3>Provider's Name and Address</h3>
                        <p>@carePlanRequestModel.AgencyName</p>
                        <p>@carePlanRequestModel.AgencyAddress</p>
                        <div class="d_flex">
                            <label>Sex: @carePlanRequestModel.PatientGender</label>
                        </div>
                    </div>
                </div>
            </div>
            <SfTab @bind-SelectedItem="SelectedPage" LoadOn="ContentLoad.Demand" CssClass="modal_SfTabs">
                <TabItems>
                    <TabItem>
                        <ChildContent>
                            <TabHeader Text="PLAN OF CARE"></TabHeader>
                        </ChildContent>
                        <ContentTemplate>
                            <h3 class="maintittle">PLAN OF CARE</h3>
                            <div class="plancare skilled_nurse_sec ">
                                <div class="row gap_10">
                                    <div class="col-md-4">
                                        <div class="row gap_10">
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label>Patient's HI Claim No</label>
                                                    <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@carePlanRequestModel.PatientClaimNo" />
                                                    <ValidationMessage For="@(() => carePlanRequestModel.PatientClaimNo)" />
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label>Start of Care Date</label>
                                                    <SfDatePicker Placeholder="MM/DD/YYYY" ShowClearButton="true" @bind-Value="@carePlanRequestModel.CareStartDate"></SfDatePicker>
                                                    <ValidationMessage For="@(() => carePlanRequestModel.CareStartDate)" />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label>Cetification Period</label>
                                            <div class="gap_10 flex_box">
                                                <div class="col-md-6">
                                                    <SfDatePicker Placeholder="MM/DD/YYYY" ShowClearButton="true" @bind-Value="@carePlanRequestModel.CertificationFrom"></SfDatePicker>
                                                    <ValidationMessage For="@(() => carePlanRequestModel.CertificationFrom)" />
                                                </div>
                                                <span class="p-0">-</span>
                                                <div class="col-md-6">
                                                    <SfDatePicker Placeholder="MM/DD/YYYY" ShowClearButton="true" @bind-Value="@carePlanRequestModel.CertificationTo"></SfDatePicker>
                                                    <ValidationMessage For="@(() => carePlanRequestModel.CertificationTo)" />
                                                </div>
                                                @if (carePlanRequestModel.CertificationFrom != null && carePlanRequestModel.CertificationTo != null)
                                                {
                                                    @if (DateTime.Compare((DateTime)carePlanRequestModel.CertificationFrom, (DateTime)carePlanRequestModel.CertificationTo) > 0)
                                                    {
                                                        <label class="validation-message">End date should be greater than start date</label>
                                                    }
                                                }
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="row gap_10">
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label>Medical Record No</label>
                                                    <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@carePlanRequestModel.MedicalRecordNo" />
                                                    <ValidationMessage For="@(() => carePlanRequestModel.MedicalRecordNo)" />
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label>Provider No</label>
                                                    <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@carePlanRequestModel.ProviderNo" />
                                                    <ValidationMessage For="@(() => carePlanRequestModel.ProviderNo)" />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <fieldset class="form-group">
                                    <legend>Eligibility(Please describe the type of service the patient will receive)</legend>
                                    <div class="multiline eli_txtarea">
                                        <SfTextBox Multiline=true FloatLabelType="@FloatLabelType.Auto" @bind-Value="@carePlanRequestModel.Eligibility"></SfTextBox>
                                    </div>
                                </fieldset>
                                <fieldset class="form-group">
                                    <legend>Diagnosis</legend>
                                    <div class="multiline">
                                        <SfTextBox Multiline=true FloatLabelType="@FloatLabelType.Always" @bind-Value="@carePlanRequestModel.DiagnosisC"></SfTextBox>
                                    </div>
                                </fieldset>
                                <fieldset class="form-group">
                                    <legend>Medications</legend>
                                    <div class="multiline">
                                        <SfTextBox Multiline=true FloatLabelType="@FloatLabelType.Auto" @bind-Value="@carePlanRequestModel.Medicationsl"></SfTextBox>
                                    </div>
                                </fieldset>
                                <fieldset class="form-group">
                                    <legend>Dme/Supplies</legend>
                                    <div class="multiline">
                                        <SfTextBox Multiline=true FloatLabelType="@FloatLabelType.Auto" @bind-Value="@carePlanRequestModel.DmeSuppliesi"></SfTextBox>
                                    </div>
                                </fieldset>
                                <fieldset class="form-group">
                                    <legend>Safety Measures</legend>
                                    <div class="multiline">
                                        <SfTextBox Multiline=true FloatLabelType="@FloatLabelType.Auto" @bind-Value="@carePlanRequestModel.SafetyMeasures"></SfTextBox>
                                    </div>
                                </fieldset>
                                <div class="row gap_10">
                                    <div class="col-md-6">
                                        <label>Date of verbal SOC</label>
                                        <div class="form-group">
                                            <SfDatePicker Placeholder="MM/DD/YYYY" ShowClearButton="true" @bind-Value="@carePlanRequestModel.VerbalSOCDate"></SfDatePicker>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Date HHA received Signed POT</label>
                                            <SfDatePicker Placeholder="MM/DD/YYYY" ShowClearButton="true" @bind-Value="@carePlanRequestModel.HHAReceivedDate"></SfDatePicker>
                                        </div>
                                    </div>
                                </div>

                                <fieldset class="form-group">
                                    <legend>Physician's Name and Address</legend>
                                    <div class="row gap_10">
                                        <div class="col-md-6">
                                            <div class="multiline">
                                                <SfTextBox Multiline=true FloatLabelType="@FloatLabelType.Auto" @bind-Value="@carePlanRequestModel.PhysicianName"></SfTextBox>
                                            </div>

                                        </div>
                                        <div class="col-md-6">
                                            <div class="multiline">
                                                <SfTextBox Multiline=true FloatLabelType="@FloatLabelType.Auto" @bind-Value="@carePlanRequestModel.PhysicianAddress"></SfTextBox>
                                            </div>
                                        </div>
                                    </div>
                                </fieldset>
                                <fieldset class="form-group">
                                    <legend>Nutritional Requirments</legend>
                                    <div class="multiline">
                                        <SfTextBox Multiline=true FloatLabelType="@FloatLabelType.Auto" @bind-Value="@carePlanRequestModel.NutritionalRequirements"></SfTextBox>
                                    </div>
                                </fieldset>
                                <fieldset class="form-group">
                                    <legend>Allergies</legend>
                                    <div class="multiline">
                                        <SfTextBox Multiline=true FloatLabelType="@FloatLabelType.Auto" @bind-Value="@carePlanRequestModel.Allergies"></SfTextBox>
                                    </div>
                                </fieldset>
                                <fieldset class="form-group">
                                    <legend>Functional Limitations</legend>
                                    <div class="multiline">
                                        <SfTextBox Multiline=true FloatLabelType="@FloatLabelType.Auto" @bind-Value="@carePlanRequestModel.FunctionalLimitations"></SfTextBox>
                                    </div>
                                </fieldset>
                                <fieldset class="form-group">
                                    <legend>Activites Permitted</legend>
                                    <div class="multiline">
                                        <SfTextBox Multiline=true FloatLabelType="@FloatLabelType.Auto" @bind-Value="@carePlanRequestModel.ActivitiesPermitted"></SfTextBox>
                                    </div>
                                </fieldset>
                                <fieldset class="form-group">
                                    <legend>MENTAL STATUS</legend>
                                    <div class="multiline">
                                        <SfTextBox Multiline=true FloatLabelType="@FloatLabelType.Auto" @bind-Value="@carePlanRequestModel.MentalStatus"></SfTextBox>
                                    </div>
                                </fieldset>
                                <fieldset class="form-group">
                                    <legend>PROGNOSIS</legend>
                                    <div class="multiline">
                                        <SfTextBox Multiline=true FloatLabelType="@FloatLabelType.Auto" @bind-Value="@carePlanRequestModel.Prognosis"></SfTextBox>
                                    </div>
                                </fieldset>
                                <fieldset class="form-group">
                                    <legend>INTERVENTIONS</legend>
                                    <div class="multiline">
                                        <SfTextBox Multiline=true FloatLabelType="@FloatLabelType.Auto" @bind-Value="@carePlanRequestModel.Interventions"></SfTextBox>
                                    </div>
                                </fieldset>
                                <fieldset class="form-group">
                                    <legend>GOALS</legend>
                                    <div class="multiline">
                                        <SfTextBox Multiline=true FloatLabelType="@FloatLabelType.Auto" @bind-Value="@carePlanRequestModel.Goals"></SfTextBox>
                                    </div>
                                </fieldset>
                                <fieldset class="form-group">
                                    <legend>ADVANCE DIRECTIVIES</legend>
                                    <div class="multiline">
                                        <SfTextBox Multiline=true FloatLabelType="@FloatLabelType.Auto" @bind-Value="@carePlanRequestModel.AdvanceDirectives"></SfTextBox>
                                    </div>
                                </fieldset>
                                <fieldset class="form-group">
                                    <legend>REHAB POTENTIAL</legend>
                                    <div class="multiline">
                                        <SfTextBox Multiline=true FloatLabelType="@FloatLabelType.Auto" @bind-Value="@carePlanRequestModel.RehabPotential"></SfTextBox>
                                    </div>
                                </fieldset>
                                <fieldset class="form-group">
                                    <legend>DISCHARGE PLANNING</legend>
                                    <div class="multiline">
                                        <SfTextBox Multiline=true FloatLabelType="@FloatLabelType.Auto" @bind-Value="@carePlanRequestModel.DischargePlanning"></SfTextBox>
                                    </div>
                                </fieldset>
                                <fieldset class="form-group">
                                    <legend>FREQUENCIES </legend>
                                    <div class="multiline">
                                        <SfTextBox Multiline=true FloatLabelType="@FloatLabelType.Auto" @bind-Value="@carePlanRequestModel.Frequencies"></SfTextBox>
                                    </div>
                                </fieldset>

                                <div class="text-center">
                                    <button type="button" @onclick="GoToCNAPlanInfoTab" class="create_account btn-theme border ">
                                        Next
                                    </button>
                                </div>
                            </div>
                        </ContentTemplate>
                    </TabItem>
                    <TabItem>
                        <ChildContent>
                            <TabHeader Text="CNA PLAN OF CARE"></TabHeader>
                        </ChildContent>
                        <ContentTemplate>
                            <div class="skilled_nurse_sec p-0">
                                <h2 class="text-center">CNA PLAN OF CARE</h2>
                                <div class="cnaplancare mt_25">
                                    <div class="row gap_10">
                                        <div class="col-md-6">
                                            <fieldset class="form-group">
                                                <label>Location Name:</label>
                                                <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@carePlanRequestModel.LocationName" />
                                                <ValidationMessage For="@(() => carePlanRequestModel.LocationName)" />
                                            </fieldset>
                                        </div>
                                        <div class="col-md-6">
                                            <fieldset class="form-group">
                                                <label>Location Number:</label>
                                                <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@carePlanRequestModel.LocationNumber" />
                                                <ValidationMessage For="@(() => carePlanRequestModel.LocationNumber)" />
                                            </fieldset>
                                        </div>
                                    </div>
                                    <div class="row gap_10">
                                        <div class="col-lg-6">
                                            <fieldset class="d_block">
                                                <legend class="transform_none">Pertinent Information</legend>
                                                <div class="flex_row_box">
                                                    <div>
                                                        <label>Code Status:</label>
                                                        <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@carePlanRequestModel.CodeStatus" />
                                                    </div>
                                                    <div>
                                                        <label>Allergies:</label>
                                                        <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@carePlanRequestModel.CnaPlanOfCareAllergies" />
                                                    </div>
                                                    <div>
                                                        <label>Diagnoses:</label>
                                                        <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@carePlanRequestModel.Diagnosis" />
                                                    </div>
                                                    <div>
                                                        <label>Diet:</label>
                                                        <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@carePlanRequestModel.Diet" />
                                                    </div>
                                                    <div>
                                                        <SfCheckBox Label="Alert" @bind-Checked="@carePlanRequestModel.IsAlert"></SfCheckBox>
                                                    </div>
                                                </div>

                                                <div>
                                                    <label>Oriented</label>
                                                    <SfCheckBox Label="Person" @bind-Checked="@carePlanRequestModel.IsOrientedPerson"></SfCheckBox>
                                                    <SfCheckBox Label="Place" @bind-Checked="@carePlanRequestModel.IsOrientedPlace"></SfCheckBox>
                                                    <SfCheckBox Label="Time" @bind-Checked="@carePlanRequestModel.IsOrientedTime"></SfCheckBox>
                                                    <SfCheckBox Label="Forgetful/Confused" @bind-Checked="@carePlanRequestModel.IsOrientedConfused"></SfCheckBox>
                                                </div>
                                                <div>
                                                    <SfCheckBox Label="Dentures:" @bind-Checked="@carePlanRequestModel.IsDentures"></SfCheckBox>
                                                    <div>
                                                        <SfCheckBox Label="Upper" @bind-Checked="@carePlanRequestModel.IsDentureUpper"></SfCheckBox>
                                                        <SfCheckBox Label="Lower" @bind-Checked="@carePlanRequestModel.IsDentureLower"></SfCheckBox>
                                                        <SfCheckBox Label="Partial" @bind-Checked="@carePlanRequestModel.IsDenturePartial"></SfCheckBox>
                                                        <SfCheckBox Label="Bed Bound" @bind-Checked="@carePlanRequestModel.IsBedBound"></SfCheckBox>
                                                        <SfCheckBox Label="Bed rest with BRP" @bind-Checked="@carePlanRequestModel.IsBedRestWithBrp"></SfCheckBox>
                                                        <SfCheckBox Label="Up as tolerated" @bind-Checked="@carePlanRequestModel.IsUpAsTolerated"></SfCheckBox>
                                                    </div>
                                                </div>
                                                <div>
                                                    <SfCheckBox Label="Hearing Deficit:" @bind-Checked="@carePlanRequestModel.IsHearingDeficit"></SfCheckBox>
                                                    <div>
                                                        <SfCheckBox Label="Upper" @bind-Checked="@carePlanRequestModel.IsHearingDeficitAid"></SfCheckBox>
                                                        <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@carePlanRequestModel.HearingAid" />
                                                        <SfCheckBox Label="Speech/communication deficit" @bind-Checked="@carePlanRequestModel.IsHearingDeficitSpeech"></SfCheckBox>
                                                    </div>
                                                </div>
                                                <div>
                                                    <SfCheckBox Label="Visiual Deficit:" @bind-Checked="@carePlanRequestModel.IsVisualDeficit"></SfCheckBox>
                                                    <div>
                                                        <SfCheckBox Label="Glasses" @bind-Checked="@carePlanRequestModel.IsVisualDeficitGlasses"></SfCheckBox>
                                                        <SfCheckBox Label="Contact" @bind-Checked="@carePlanRequestModel.IsVisualDeficitContacts"></SfCheckBox>
                                                        <SfCheckBox Label="Other" @bind-Checked="@carePlanRequestModel.IsVisualDeficitOther"></SfCheckBox>
                                                        <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" />
                                                    </div>
                                                </div>
                                                <div>
                                                    <SfCheckBox Label="Assitive device:" @bind-Checked="@carePlanRequestModel.IsAssistiveDevices"></SfCheckBox>
                                                    <div>
                                                        <SfCheckBox Label="WC" @bind-Checked="@carePlanRequestModel.IsAssistiveDevicesWC"></SfCheckBox>
                                                        <SfCheckBox Label="Walker" @bind-Checked="@carePlanRequestModel.IsAssistiveDevicesWalker"></SfCheckBox>
                                                        <SfCheckBox Label="Cane" @bind-Checked="@carePlanRequestModel.IsAssistiveDevicesCane"></SfCheckBox>
                                                        <SfCheckBox Label="Crutches" @bind-Checked="@carePlanRequestModel.IsAssistiveDevicesCrutches"></SfCheckBox>
                                                        <SfCheckBox Label="Brance/Splint" @bind-Checked="@carePlanRequestModel.IsAssistiveDevicesBraceSplint"></SfCheckBox>
                                                        <SfCheckBox Label="Others" @bind-Checked="@carePlanRequestModel.IsAssistiveDevicesOther"></SfCheckBox>
                                                        <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@carePlanRequestModel.AssistiveDevicesOther" />
                                                    </div>
                                                </div>
                                                <div>
                                                    <SfCheckBox Label="Others" @bind-Checked="@carePlanRequestModel.IsPertientInformationOther"></SfCheckBox>
                                                    <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="carePlanRequestModel.PertientInformationOther" />
                                                </div>
                                            </fieldset>
                                        </div>
                                        <div class="col-lg-6">
                                            <fieldset>
                                                <legend class="transform_none">Observation to Report to Supervisior/Case Manager</legend>
                                                <table class="table table-striped table-bordered">
                                                    <tbody>
                                                        <tr>
                                                            <td><SfCheckBox Label="Temperature" @bind-Checked="@carePlanRequestModel.IsObserveTemp"></SfCheckBox></td>
                                                            <td>
                                                                <div class="table_report">
                                                                    <SfCheckBox Label="Over:" @bind-Checked="@carePlanRequestModel.IsObserveTempOver"></SfCheckBox>
                                                                    <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@carePlanRequestModel.ObserveTempOver" />
                                                                </div>
                                                            </td>
                                                            <td>
                                                                <div class="table_report">
                                                                    <SfCheckBox Label="Under:" @bind-Checked="@carePlanRequestModel.IsObserveTempUnder"></SfCheckBox>
                                                                    <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@carePlanRequestModel.ObserveTempUnder" />
                                                                </div>
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td><SfCheckBox Label="Pulse" @bind-Checked="@carePlanRequestModel.IsObservePulse"></SfCheckBox></td>
                                                            <td>
                                                                <div class="table_report">
                                                                    <SfCheckBox Label="Over:" @bind-Checked="@carePlanRequestModel.IsObservePulseOver"></SfCheckBox>
                                                                    <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@carePlanRequestModel.ObservePulseOver" />
                                                                </div>
                                                            </td>
                                                            <td>
                                                                <div class="table_report">
                                                                    <SfCheckBox Label="Under:" @bind-Checked="@carePlanRequestModel.IsObservePulseUnder"></SfCheckBox>
                                                                    <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@carePlanRequestModel.ObservePulseUnder" />
                                                                </div>
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td><SfCheckBox Label="Respirations" @bind-Checked="@carePlanRequestModel.IsObserveRespiration"></SfCheckBox></td>
                                                            <td>
                                                                <div class="table_report">
                                                                    <SfCheckBox Label="Over:" @bind-Checked="@carePlanRequestModel.IsObserveRespirationOver"></SfCheckBox>
                                                                    <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@carePlanRequestModel.ObserveRespirationOver" />
                                                                </div>
                                                            </td>
                                                            <td>
                                                                <div class="table_report">
                                                                    <SfCheckBox Label="Under:" @bind-Checked="@carePlanRequestModel.IsObserveRespirationUnder"></SfCheckBox>
                                                                    <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@carePlanRequestModel.ObserveRespirationUnder" />
                                                                </div>
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td><SfCheckBox Label="Blood Pressure" @bind-Checked="@carePlanRequestModel.IsObserveBP"></SfCheckBox></td>
                                                            <td>
                                                                <div class="table_report">
                                                                    <SfCheckBox Label="Over:" @bind-Checked="@carePlanRequestModel.IsObserveBPOver"></SfCheckBox>
                                                                    <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@carePlanRequestModel.ObserveBPOver" />
                                                                </div>
                                                            </td>
                                                            <td>
                                                                <div class="table_report">
                                                                    <SfCheckBox Label="Under:" @bind-Checked="@carePlanRequestModel.IsObserveBPUnder"></SfCheckBox>
                                                                    <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@carePlanRequestModel.ObserveBPUnder" />
                                                                </div>
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td><SfCheckBox Label="Intake amount" @bind-Checked="@carePlanRequestModel.IsObserveIntake"></SfCheckBox></td>
                                                            <td>
                                                                <div class="table_report">
                                                                    <SfCheckBox Label="Over:" @bind-Checked="@carePlanRequestModel.IsObserveIntakeOver"></SfCheckBox>
                                                                    <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@carePlanRequestModel.ObserveIntakeOver" />
                                                                </div>
                                                            </td>
                                                            <td>
                                                                <div class="table_report">
                                                                    <SfCheckBox Label="Under:" @bind-Checked="@carePlanRequestModel.IsObserveIntakeUnder"></SfCheckBox>
                                                                    <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@carePlanRequestModel.ObserveIntakeUnder" />
                                                                </div>
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td><SfCheckBox Label="Output amount" @bind-Checked="@carePlanRequestModel.IsObserveOutputAmount"></SfCheckBox></td>
                                                            <td>
                                                                <div class="table_report">
                                                                    <SfCheckBox Label="Over:" @bind-Checked="@carePlanRequestModel.IsObserveOutputAmountOver"></SfCheckBox>
                                                                    <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@carePlanRequestModel.ObserveOutputAmountOver" />
                                                                </div>
                                                            </td>
                                                            <td>
                                                                <div class="table_report">
                                                                    <SfCheckBox Label="Under:" @bind-Checked="@carePlanRequestModel.IsObserveOutputAmountUnder"></SfCheckBox>
                                                                    <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@carePlanRequestModel.ObserveOutputAmountUnder" />
                                                                </div>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                                <div>
                                                    <div>
                                                        <SfCheckBox Label="No BM in days" @bind-Checked="@carePlanRequestModel.IsObserveNoBMInDays"></SfCheckBox>
                                                    </div>
                                                    <div>
                                                        <SfCheckBox Label="Pain above:" @bind-Checked="@carePlanRequestModel.IsObservePainAbove"></SfCheckBox>
                                                        <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@carePlanRequestModel.ObservePainAbove" />
                                                    </div>
                                                    <SfCheckBox Label="Change in skin:" @bind-Checked="@carePlanRequestModel.IsObserveChangesInSkin"></SfCheckBox>
                                                    <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@carePlanRequestModel.ObserveChangesInSkin" />
                                                    <SfCheckBox Label="Reddened area" @bind-Checked="@carePlanRequestModel.IsObserveReddenedArea"></SfCheckBox>
                                                    <SfCheckBox Label="Others:" @bind-Checked="@carePlanRequestModel.IsObserveReddenedOther"></SfCheckBox>
                                                    <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@carePlanRequestModel.ObserveReddenedOther" />
                                                </div>
                                                <div>
                                                    <SfCheckBox Label="Weight" @bind-Checked="@carePlanRequestModel.IsObserveWeight"></SfCheckBox>
                                                    <SfCheckBox Label="Gain" @bind-Checked="@carePlanRequestModel.IsObserveWeightGain"></SfCheckBox>
                                                    <SfCheckBox Label="loss of lb over(time period)" @bind-Checked="@carePlanRequestModel.IsObserveWeightLoss"></SfCheckBox>
                                                    <SfTextBox FloatLabelType="@FloatLabelType.Auto" Type="InputType.Text" @bind-Value="@carePlanRequestModel.ObserveWeightLoss" />
