@code {
    int SelectedPage { get; set; } = 0;
    CarePlanRequestModel carePlanRequestModel = new CarePlanRequestModel();
    bool isLoading = false;
    [Parameter]
    public int Id { get; set; }
    [Parameter]
    public int JobId  { get; set; }
    [Parameter]
    public bool View { get; set; }
    AdultAssessmentRequestModel intitalAssessmentRequestModel = new AdultAssessmentRequestModel();
    DiagnosisViewModel diagnosisRequestViewModel = new DiagnosisViewModel();
    [Parameter]
    public string Role { get; set; }

    protected override async Task OnInitializedAsync()
    {
        var responseMessage = await Http.GetAsync(wsp.Path + $"CarePlan/GetCarePlanById?id={Id}");
        var response = responseMessage.Content.ReadAsStringAsync().Result;
        var result = (JsonConvert.DeserializeObject<Response<CarePlanRequestModel>>(response));
        if (result.Status == "Success")
        {
            carePlanRequestModel = result.Data;
        }
        else
        {
            toastService.ShowError(result.Message);
            NavigateToCarePlansGrid();
        }

        var responseMessages = await Http.GetAsync(wsp.Path + $"AdultAssessment/GetAssessmentByJobId?id={carePlanRequestModel.JobId}");
        var responses = responseMessages.Content.ReadAsStringAsync().Result;
        var results = (JsonConvert.DeserializeObject<Response<AdultAssessmentRequestModel>>(responses));
        if (result.Status == "Success")
        {
            intitalAssessmentRequestModel = results.Data;
            carePlanRequestModel.SafetyMeasures = intitalAssessmentRequestModel.SafetyMeasuresRecommended;
            carePlanRequestModel.Eligibility = intitalAssessmentRequestModel.FrequencyEligibility;
            carePlanRequestModel.Prognosis = intitalAssessmentRequestModel.FrequencyPrognosis;
            carePlanRequestModel.DmeSuppliesi = intitalAssessmentRequestModel.FrequencyDMESupplies;     
            carePlanRequestModel.Diet = intitalAssessmentRequestModel.NutritionalRequirements;
            carePlanRequestModel.FunctionalLimitations = intitalAssessmentRequestModel.FunctionalLimitations;
            carePlanRequestModel.ActivitiesPermitted = intitalAssessmentRequestModel.ActivitiesPermitted;
            carePlanRequestModel.MentalStatus = intitalAssessmentRequestModel.MaritalStatus;
            carePlanRequestModel.Prognosis = intitalAssessmentRequestModel.FrequencyPrognosis;
            carePlanRequestModel.Interventions = intitalAssessmentRequestModel.InterventionsEducation;
            carePlanRequestModel.Goals = intitalAssessmentRequestModel.FrequencyGoals;
            carePlanRequestModel.AdvanceDirectives = intitalAssessmentRequestModel.IsAwareAdvanceDirective;
            carePlanRequestModel.DischargePlanning = intitalAssessmentRequestModel.DischargePlanning;
            carePlanRequestModel.Frequencies = intitalAssessmentRequestModel.FrequencySN;
            carePlanRequestModel.PhysicianName = intitalAssessmentRequestModel.ReferringPhysician;
            carePlanRequestModel.PhysicianAddress = intitalAssessmentRequestModel.PhysicianAddress;
            carePlanRequestModel.Eligibility = intitalAssessmentRequestModel.FrequencyEligibility;
            List<string> diagnosis = new List<string>();
            foreach (var item in intitalAssessmentRequestModel.Diagnosis)
            {
                diagnosis.Add(item.DxDescription);
                diagnosis.Add(item.DxDate.ToString());
            }
            carePlanRequestModel.DiagnosisC = String.Join(Environment.NewLine, diagnosis);
        }
        else
        {
            toastService.ShowError(result.Message);
        }
    }

    public async Task UpdateCarePlan()
    {
        isLoading = true;
        var responseMessage = await Http.PostAsJsonAsync(wsp.Path + "CarePlan/UpdateCarePlan", carePlanRequestModel);
        var response = responseMessage.Content.ReadAsStringAsync().Result;
        var result = (JsonConvert.DeserializeObject<Response<string>>(response));
        if (result.Status == "Success")
        {
            toastService.ShowSuccess(result.Message);
            NavigateToCarePlansGrid();
        }
        else
        {
            toastService.ShowError(result.Message);
        }
        isLoading = false;
        StateHasChanged();
    }

    public void NavigateToCarePlansGrid()
    {
        if (Role == UserRoles.Nurse.ToString())
            navigation.NavigateTo("nurse/care-plans");
        else
            navigation.NavigateTo("agency/care-plans");
    }

    public void GoToCNAPlanInfoTab()
    {
        SelectedPage = 1;
        StateHasChanged();
    }

    public void GoToPlanInfoTab()
    {
        SelectedPage = 0;
        StateHasChanged();
    }

    public void OnPersonalCareBathBedChange()
    {
        if (!carePlanRequestModel.IsPersonalCareBathBed)
        {
            carePlanRequestModel.IsPersonalCareBathPartial = null;
        }
        StateHasChanged();
    }
}


